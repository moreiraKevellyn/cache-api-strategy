    "test:high": "artillery run scripts/load-tests/high-load.yml"
  },
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "node-cache": "^5.1.2",
    "lru-cache": "^10.0.1",
    "prom-client": "^15.0.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/node": "^20.9.0",
    "@types/pg": "^8.10.7",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "artillery": "^2.0.3"
  }
